{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "HTML Dateien erzeugen aus Vorlage",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/html/create_html_clones.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/venv/Scripts/python.exe",
            "env": {},
            "justMyCode": true
        },
        {
            "name": "Django - makemigrations",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "makemigrations",
            ],
            "django": true,
            "python": "${workspaceFolder}/venv/Scripts/python.exe",
            "env": {},
            "justMyCode": true
        },
        {
            "name": "Django - migrate",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "migrate",
            ],
            "django": true,
            "python": "${workspaceFolder}/venv/Scripts/python.exe",
            "env": {},
            "justMyCode": true
        },
        {
            "name": "Django - runserver",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "0.0.0.0:8000"
            ],
            "django": true,
            "python": "${workspaceFolder}/venv/Scripts/python.exe",
            "env": {},
            "justMyCode": true
        },
        {
            "name": "Python: Aktuelles Modul",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/venv/Scripts/python.exe",
            "env": {},
            "justMyCode": true
        },
    ]
}